digraph {
    bgcolor=transparent;
    rankdir=LR;

    node [shape=none];

    a [label=<<i>a</i>>];
    b [label=<<i>b</i>>];
    p [label=<<i>p</i>>, shape=box];
    pOut [shape=point, width=0.001];

    a       -> p;
    p       -> pOut [dir=none];
    pOut    -> b    [tailport=e];
    pOut    -> b    [tailport=e];
    pOut    -> b    [tailport=e];
}
